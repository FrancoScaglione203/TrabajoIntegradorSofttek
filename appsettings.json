{
  "ConnectionStrings": {
    "defaultConnection": "Server=DESKTOP-NVMM4F9\\SQLEXPRESS; Database=TrabajoIntegradorSofttek; User Id=sa; Password=1234; TrustServerCertificate=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Key": "KeyProyectoIntegrador1234",
    "Issuer": "https://localhost:7112",
    "Audience": "https://localhost:7112",
    "subject": "WebApiSubject"
  },
  "AllowedHosts": "*",
  "Serilog": {
    "LevelSwitches": {
      "$consoleSwitch": "Debug",
      "$fileVerboseSwitch": "Verbose",
      "$fileWarningSwitch": "Warning"
    },
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "levelSwitch": "$consoleSwitch"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log.verbose.txt",
          "levelSwitch": "$fileVerboseSwitch"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log.warning.txt",
          "levelSwitch": "$fileWarningSwitch"
        }
      }
    ]
  }
}
